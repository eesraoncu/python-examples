def transpose(matrix:list):
    new_matrix=[]
    for row in matrix:
        copied_row=row[:]
        new_matrix.append(copied_row)
    for i in range(len(matrix)):
        for j in range(len(matrix[0])):
            matrix[j][i]=new_matrix[i][j]
my_matrix=[[1,2,3],[4,5,6],[7,8,9]]
transpose(my_matrix)
print(my_matrix)
