phone_book={}
while True:
    command = input("Enter command(1 search, 2 add, 3 quit): ")
    if command=="1":
        name = input("name: ")
        if name in phone_book:
            print(phone_book[name])
        else:
            print("no number")
    elif command=="2":
     name=input("Enter name:")
     number=int(input("Enter number:"))
     phone_book[name]=number
     print("ok")
    elif command=="3":
     print("quitting")
     break
